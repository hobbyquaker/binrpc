{
  "version": "3.1.0",
  "name": "binrpc",
  "description": "HomeMatic binary RPC protocol - xmlrpc_bin://",
  "homepage": "https://github.com/hobbyquaker/binrpc",
  "author": "Sebastian 'hobbyquaker' Raff <hq@ccu.io>",
  "keywords": [
    "HomeMatic",
    "binrpc",
    "xmlrpc_bin",
    "BidCos",
    "CUxD",
    "Homegear",
    "rfd",
    "hs485d"
  ],
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/hobbyquaker/binrpc.git"
  },
  "main": "./lib/binrpc.js",
  "dependencies": {
    "binary": "^0.3.0",
    "put": "0.0.6"
  },
  "bugs": {
    "url": "https://github.com/hobbyquaker/binrpc/issues"
  },
  "scripts": {
    "test": "xo lib/* && nyc mocha --exit tests/* && nyc report --reporter=text-lcov | coveralls --force"
  },
  "license": "MIT",
  "devDependencies": {
    "camo-purge": "latest",
    "coveralls": "latest",
    "grunt": "latest",
    "grunt-cli": "latest",
    "grunt-contrib-concat": "latest",
    "grunt-jsdoc-to-markdown": "latest",
    "grunt-shell": "latest",
    "grunt-simple-mocha": "latest",
    "grunt-xo": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "should": "latest",
    "xo": "latest"
  },
  "directories": {
    "test": "tests"
  },
  "xo": {
    "esnext": false,
    "space": 4
  },
  "nyc": {
    "exclude": [
      "tests/*.js",
      "example.js",
      "Gruntfile.js"
    ]
  },
  "engines": {
    "node": ">4.0.0"
  }
